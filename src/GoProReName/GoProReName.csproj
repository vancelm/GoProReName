<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Version>0.0.0.0</Version>
    <Authors>Vance Morgan</Authors>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <Description>Renames GoPro video files so they are easier to organize.</Description>
    <Copyright>Copyright (c) 2020 Vance Morgan</Copyright>
    <PackageProjectUrl>https://github.com/vancelm/GoProReName</PackageProjectUrl>
    <RepositoryUrl>https://github.com/vancelm/GoProReName</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <AssemblyName>gprn</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CommandLineParser" Version="2.7.82" />
  </ItemGroup>

</Project>
